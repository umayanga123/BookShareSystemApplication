<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="469.0" prefWidth="691.0" styleClass="login-form" stylesheets="@designLogin.css" BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.FXMLDocumentController">
   <children>
      <AnchorPane layoutX="90.0" layoutY="79.0" prefHeight="336.0" prefWidth="491.0" styleClass="card-form" stylesheets="@designLogin.css">
         <children>
            <Label layoutX="199.0" layoutY="54.0" prefHeight="17.0" prefWidth="93.0" text="Login" textFill="#f8dada">
               <font>
                  <Font name="Arial Bold Italic" size="25.0" />
               </font>
            </Label>
            <TextField fx:id="studentNumber" layoutX="156.0" layoutY="104.0" onKeyTyped="#numbersOnly" prefHeight="25.0" prefWidth="180.0" promptText="Stunent Number" styleClass="textfield" stylesheets="@designLogin.css" />
            <PasswordField fx:id="password" layoutX="156.0" layoutY="149.0" prefHeight="25.0" prefWidth="180.0" promptText="Password" styleClass="textfield" stylesheets="@designLogin.css" />
            <Button fx:id="login_Btn" layoutX="223.0" layoutY="205.0" mnemonicParsing="false" onAction="#login" styleClass="login-btn" stylesheets="@designLogin.css" text="Login" />
         </children></AnchorPane>
      <Button fx:id="minimize" layoutX="644.0" layoutY="2.0" mnemonicParsing="false" onAction="#minimize" styleClass="minimize" stylesheets="@designLogin.css" text="_" textFill="#e4dcdc">
         <font>
            <Font name="System Bold" size="10.0" />
         </font></Button>
      <Button fx:id="close" layoutX="665.0" layoutY="2.0" mnemonicParsing="false" onAction="#exit" styleClass="close" stylesheets="@designLogin.css" text="X" textFill="#e1dcc7">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
   </children>
</AnchorPane>
